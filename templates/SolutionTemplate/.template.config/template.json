{
  "$schema": "http://json.schemastore.org/template",
  "author": "James McSweeny",
  "identity": "SolutionTemplate",
  "name": "F# Solution Day Template",
  "shortName": "fs-solution-template",
  "tags": { "language": "F#", "type": "project" },

  "symbols": {
    "year": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "$year$",
      "fileRename": "$year$"
    },
    "day": {
      "type": "parameter",
      "datatype": "string",
      "replaces": "$day$",
      "fileRename": "$day$"
    }
  },

  "sources": [
    {
      "source": "./Solutions",
      "target": "Solutions"
    },
    {
      "source": "./Input",
      "target": "Input"
    }
  ]
}